	Issn = {0926860X},
	Journal = {Applied Catalysis A: General},
	Keywords = {combinatorial catalysis,evolutionary catalyst optimization approach,propane oxidative dehydrogenation,solid mixed metal, evolutionary algorithm, materials design, crystal structure prediction, inverse design},
	Number = {1-2},
	Pages = {63--77},
	Title = {{An evolutionary approach in the combinatorial selection and optimization of catalytic materials}},
