	Isbn = {1359-6454},
	Issn = {13596454},
	Journal = {Acta Materialia},
	Keywords = {Cellular solids,Metal foam,Open-cell foam,Reverse engineering,X-ray computed tomography},
	Pages = {387--395},
	Title = {{The effect of density and feature size on mechanical properties of isostructural metallic foams produced by additive manufacturing}},
	Volume = {85},
	Year = {2015},
