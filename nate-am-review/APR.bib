@article{Long2009,
	Abstract = {In this work we apply a technique called non-negative matrix factorization (NMF) to the problem of analyzing hundreds of x-ray microdiffraction (microXRD) patterns from a combinatorial materials library. An in-house scanning x-ray microdiffractometer is used to obtain microXRD patterns from 273 different compositions on a single composition spread library. NMF is then used to identify the unique microXRD patterns present in the system and quantify the contribution of each of these basis patterns to each experimental diffraction pattern. As a baseline, the results of NMF are compared to the results obtained using principle component analysis. The basis patterns found using NMF are then compared to reference patterns from a database of known structural patterns in order to identify known structures. As an example system, we explore a region of the Fe-Ga-Pd ternary system. The use of NMF in this case reduces the arduous task of analyzing hundreds of microXRD patterns to the much smaller task of identifying only nine microXRD patterns.},
	Author = {Long, C. J. and Bunker, D. and Li, X. and Karen, V. L. and Takeuchi, I.},
	Doi = {10.1063/1.3216809},
	File = {:Users/njohnson/Documents/Articles/Applied Physics Review/Long{\_}Rapid identification of structural phases in combinatorial thin-film libraries using x-ray diffraction and non-negative matrix factorization.pdf:pdf},
	Issn = {00346748},
	Journal = {Review of Scientific Instruments},
	Number = {10},
	Pmid = {19895071},
	Title = {{Rapid identification of structural phases in combinatorial thin-film libraries using x-ray diffraction and non-negative matrix factorization}},
	Volume = {80},
	Year = {2009},
	Bdsk-Url-1 = {http://dx.doi.org/10.1063/1.3216809}}

@article{Long2007,
	Abstract = {We are developing a procedure for the quick identification of structural phases in thin film composition spread experiments which map large fractions of compositional phase diagrams of ternary metallic alloy systems. An in-house scanning x-ray microdiffractometer is used to obtain x-ray spectra from 273 different compositions on a single composition spread library. A cluster analysis software is then used to sort the spectra into groups in order to rapidly discover the distribution of phases on the ternary diagram. The most representative pattern of each group is then compared to a database of known structures to identify known phases. Using this method, the arduous analysis and classification of hundreds of spectra is reduced to a much shorter analysis of only a few spectra. (c) 2007 American Institute of Physics.},
	Author = {Long, C. J. and Hattrick-Simpers, J. and Murakami, M. and Srivastava, R. C. and Takeuchi, I. and Karen, V. L. and Li, X.},
